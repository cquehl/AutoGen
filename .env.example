# Azure OpenAI Configuration (Default Provider)
AZURE_OPENAI_API_KEY=your_azure_api_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/
AZURE_OPENAI_DEPLOYMENT_NAME=gpt-4o

# OpenAI Configuration (Alternative)
# OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini Configuration (Alternative)
# GOOGLE_API_KEY=your_google_api_key_here

# Database Configuration (for Data Agent)
# SQLite (local file)
DATABASE_URL=sqlite:///./data/mydata.db

# PostgreSQL
# DATABASE_URL=postgresql://username:password@localhost:5432/mydatabase

# MySQL
# DATABASE_URL=mysql://username:password@localhost:3306/mydatabase

# Optional: IP Address Check (for security)
# ALLOWED_IP=123.456.789.012
